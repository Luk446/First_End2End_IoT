    {
        "id": "c31f0811.b2031",
        "type": "function",
        "z": "572a4fbb.49dd48",
        "name": "Create a Chart Array for Chart.js",
        "func": "// Convert Mongo Extended JSON docs -> ui_chart batch for Temp & Humidity\n// Example doc:\n// {\n//   payload: {\n//     time: { \"$numberDouble\": \"1762381382190.0\" },\n//     temp: { \"$numberInt\": \"19\" },\n//     humidity: { \"$numberInt\": \"65\" }\n//   }\n// }\n\nconst docs = Array.isArray(msg.payload) ? msg.payload : [];\nif (!docs.length) {\n    msg.payload = [];\n    return msg;\n}\n\n// Helper: unwrap Mongo Extended JSON numeric fields\nfunction getNum(v) {\n    if (v == null) return NaN;\n    if (typeof v === \"number\") return v;\n    if (typeof v === \"string\") return Number(v);\n    if (typeof v === \"object\") {\n        if (\"$numberDouble\" in v) return Number(v.$numberDouble);\n        if (\"$numberInt\" in v) return Number(v.$numberInt);\n        if (\"$numberLong\" in v) return Number(v.$numberLong);\n        if (\"$date\" in v) return Number(v.$date);\n    }\n    return NaN;\n}\n\n// Build arrays for both temperature and humidity\nconst tempPoints = [];\nconst humPoints = [];\n\nfor (const d of docs) {\n    const t = getNum(d?.payload?.time);\n    const temp = getNum(d?.payload?.temp);\n    const hum = getNum(d?.payload?.humidity);\n\n    if (!Number.isFinite(t)) continue; // skip invalid times\n    const ms = t < 1e12 ? t * 1000 : t; // convert seconds to ms if needed\n\n    if (Number.isFinite(temp)) tempPoints.push({ x: ms, y: temp });\n    if (Number.isFinite(hum)) humPoints.push({ x: ms, y: hum });\n}\n\n// Sort both by time\ntempPoints.sort((a, b) => a.x - b.x);\nhumPoints.sort((a, b) => a.x - b.x);\n\n// Check if we have any data\nif (!tempPoints.length && !humPoints.length) {\n    node.warn(\"No valid data points found in MongoDB results.\");\n    msg.payload = [];\n    return msg;\n}\n\n// Format for Node-RED dashboard Chart (Chart.js)\nmsg.payload = [\n    {\n        series: [\"Temperature (Â°C)\", \"Humidity (%)\"],\n        data: [tempPoints, humPoints],\n        labels: [\"Temp\", \"Humidity\"]\n    }\n];\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 670,
        "y": 440,
        "wires": [
            [
                "958dd94a.779118",
                "64d728269dcc5f19"
            ]
        ]
    },
